{
    "compilerOptions": {
       "lib": [
          "es6"
       ],
       "target": "ESNext",
       "module": "ESNext",
       "moduleResolution": "node",
       "outDir": "dist",
       "resolveJsonModule": true,
       "emitDecoratorMetadata": true,
       "esModuleInterop": true,
       "experimentalDecorators": true,
       "sourceMap": true,
       "noEmit": true,
       "allowImportingTsExtensions": true
    },
    "include": ["*.ts"],
    "exclude": ["node_modules"],
 }